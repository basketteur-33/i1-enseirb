#include <stdlib.h>
#include <stdio.h>


int fibo_rec(int n) {
  if (n == 0)
    return 0;

  if (n == 1)
    return 1;

  return fibo_rec(n-1) + fibo_rec(n-2);
}


int main(int argc, char *argv[]) {
  for (int i = 0 ; i < 10 ; i++)
    printf("fibo_rec(%d) : %d\n", i, fibo_rec(i));

  return EXIT_SUCCESS;
}
