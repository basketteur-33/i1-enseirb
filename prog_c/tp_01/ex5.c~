#include <stdlib.h>
#include <stdio.h>


void pascal_ligne(int n, int valeurs[]) {
  int i = 1;

  printf("%d ", 1);

  while (i < n - 1) {
    printf("%d ", valeurs[i-1] + valeurs[i]);
    i++;
  }

  printf("%d\n", 1);
}


int main(int argc, char *argv[]) {
  int t[] = {1, 2, 1};
  pascal_ligne(4, t);

  return EXIT_SUCCESS;
}
